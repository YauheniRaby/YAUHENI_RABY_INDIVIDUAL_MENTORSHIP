{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss MM.dd.yyyy ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AppConfiguration": {
    "MinCountDaysForecast": 0,
    "MaxCountDaysForecast": 5,
    "IsDebugMode": true,
    "RequestTimeout": 1000000
  },
  "IdentityUrl": "https://localhost:6001"
}
