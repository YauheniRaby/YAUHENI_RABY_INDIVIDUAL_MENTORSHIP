{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterOptions": {
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss MM.dd.yyyy ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*",
  "AppParams": {
    "MinCountDaysForecast": 0,
    "MaxCountDaysForecast": 5,
    "IsDebugMode": true,
    "RequestTimeout": 1000000
  }
}
